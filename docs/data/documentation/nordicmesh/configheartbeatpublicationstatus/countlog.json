{"metadata":{"externalID":"s:10NordicMesh32ConfigHeartbeatPublicationStatusV8countLogs5UInt8Vvp","modules":[{"name":"NordicMesh"}],"role":"symbol","symbolKind":"property","fragments":[{"kind":"keyword","text":"let"},{"text":" ","kind":"text"},{"text":"countLog","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier","text":"UInt8"}],"roleHeading":"Instance Property","title":"countLog"},"abstract":[{"type":"text","text":"Number of Heartbeat messages remaining to be sent."}],"schemaVersion":{"patch":0,"major":0,"minor":3},"primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"text":"let","kind":"keyword"},{"kind":"text","text":" "},{"text":"countLog","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier","text":"UInt8"}],"platforms":["macOS"]}],"kind":"declarations"},{"content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"inlineContent":[{"text":"Possible values:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"0x00 - Periodic Heartbeat messages are not published.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"0x01 - 0x11 - Number of Heartbeat messages, 2^(n-1), that remain to be sent.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0xFF - Periodic Heartbeat messages are published sent indefinitely."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Other values are prohibited.","type":"text"}]}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"text":"The Heartbeat Publication Count Log is a representation of the Heartbeat Publication","type":"text"},{"type":"text","text":" "},{"text":"Count state value. The Heartbeat Publication Count Log and Heartbeat Publication Count","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with the value 0x00 and 0x0000 are equivalent. The Heartbeat Publication Count Log"},{"type":"text","text":" "},{"text":"value of 0xFF is equivalent to the Heartbeat Publication count value of 0xFFFF.","type":"text"},{"text":" ","type":"text"},{"text":"The Heartbeat Publication Count Log value between 0x01 and 0x11 shall represent that","type":"text"},{"type":"text","text":" "},{"text":"smallest integer n where 2^(n-1) is greater than or equal to the Heartbeat Publication","type":"text"},{"type":"text","text":" "},{"text":"Count value. For example, if the Heartbeat Publication Count value is 0x0579, then the","type":"text"},{"type":"text","text":" "},{"text":"Heartbeat Publication Count Log value would be 0x0C.","type":"text"}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog"]}],"sections":[],"identifier":{"url":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus\/countLog","interfaceLanguage":"swift"},"hierarchy":{"paths":[["doc:\/\/NordicMesh\/documentation\/NordicMesh","doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus"]]},"kind":"symbol","references":{"doc://NordicMesh/documentation/NordicMesh/ConfigHeartbeatPublicationStatus/countLog":{"identifier":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus\/countLog","abstract":[{"type":"text","text":"Number of Heartbeat messages remaining to be sent."}],"url":"\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog","type":"topic","role":"symbol","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"countLog"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"UInt8","preciseIdentifier":"s:s5UInt8V"}],"title":"countLog","kind":"symbol"},"doc://NordicMesh/documentation/NordicMesh":{"abstract":[{"text":"Provision, configure and control Bluetooth mesh devices with nRF Mesh libr