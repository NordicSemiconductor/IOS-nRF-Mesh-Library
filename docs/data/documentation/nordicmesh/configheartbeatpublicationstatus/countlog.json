{"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog"]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/NordicMesh\/documentation\/NordicMesh","doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus"]]},"sections":[],"metadata":{"role":"symbol","roleHeading":"Instance Property","symbolKind":"property","modules":[{"name":"NordicMesh"}],"externalID":"s:10NordicMesh32ConfigHeartbeatPublicationStatusV8countLogs5UInt8Vvp","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"countLog","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier","text":"UInt8"}],"title":"countLog"},"abstract":[{"text":"Number of Heartbeat messages remaining to be sent.","type":"text"}],"kind":"symbol","identifier":{"interfaceLanguage":"swift","url":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus\/countLog"},"primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"countLog","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier","text":"UInt8"}],"platforms":["macOS"]}],"kind":"declarations"},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Possible values:","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"0x00 - Periodic Heartbeat messages are not published."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"0x01 - 0x11 - Number of Heartbeat messages, 2^(n-1), that remain to be sent."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0xFF - Periodic Heartbeat messages are published sent indefinitely."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Other values are prohibited."}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The Heartbeat Publication Count Log is a representation of the Heartbeat Publication"},{"type":"text","text":" "},{"type":"text","text":"Count state value. The Heartbeat Publication Count Log and Heartbeat Publication Count"},{"type":"text","text":" "},{"text":"with the value 0x00 and 0x0000 are equivalent. The Heartbeat Publication Count Log","type":"text"},{"type":"text","text":" "},{"type":"text","text":"value of 0xFF is equivalent to the Heartbeat Publication count value of 0xFFFF."},{"type":"text","text":" "},{"type":"text","text":"The Heartbeat Publication Count Log value between 0x01 and 0x11 shall represent that"},{"type":"text","text":" "},{"type":"text","text":"smallest integer n where 2^(n-1) is greater than or equal to the Heartbeat Publication"},{"text":" ","type":"text"},{"type":"text","text":"Count value. For example, if the Heartbeat Publication Count value is 0x0579, then the"},{"type":"text","text":" "},{"text":"Heartbeat Publication Count Log value would be 0x0C.","type":"text"}]}],"kind":"content"}],"references":{"doc://NordicMesh/documentation/NordicMesh/ConfigHeartbeatPublicationStatus/countLog":{"type":"topic","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"Number of Heartbeat messages remaining to be sent."}],"title":"countLog","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"countLog","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt8","preciseIdentifier":"s:s5UInt8V"}],"url":"\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog","identifier":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus\/countLog"},"doc://NordicMesh/documentation/NordicMesh":{"abstract":[{"text":"Provision, configure and control Bluetooth mesh devices with nRF Mesh libr